# !/bin/bash
# First argument: number of nodes

if [ $# -eq 0 ]
  then
    echo "Please provide number of nodes as argument"
		echo "./creategraph <Num Nodes>"
		exit 1
fi

for i in $(seq 0 "$(($1-1))");
do
	  ./txt2Png ../../../build/scratch/b4mesh/Traces/blockgraph-$i.txt ../results/graph-$i.png
    ./txt2Png_time ../../../build/scratch/b4mesh/Traces/blockgraph-$i.txt ../results/graphTime-$i.png
done

echo "Results availables in ../results/"
